                    / react-empty: 86
                  .form-group
                    %label
                      / react-text: 89
                      発送までの日数
                      / /react-text
                      %span.form-require 必須
                    .select-wrap
                      %i.fa.fa-angle-down
                      %select.select-default
                        %option{:value => ""} ---
                        %option{:value => "1"} 1~2日で発送
                        %option{:value => "1"} 2~3日で発送
